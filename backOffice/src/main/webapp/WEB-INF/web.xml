<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>backOffice</display-name>
    <servlet>
        <servlet-name>ProjetServlet</servlet-name>
        <servlet-class>fr.utbm.backoffice.ProjetServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletInscription</servlet-name>
        <servlet-class>fr.utbm.backoffice.ServletInscription</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProjetServlet</servlet-name>
        <url-pattern>/ProjetServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletInscription</servlet-name>
        <url-pattern>/ServletInscription</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
